{
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/src/index.html" },
      { "src": "/signin", "dest": "/src/signin.html" },
      { "src": "/signup", "dest": "/src/signup.html" },
      { "src": "/reset-password", "dest": "/src/reset-password.html" },
      { "src": "/signup-with-email", "dest": "/src/signup-with-email.html" },
      { "src": "/(.*)", "dest": "/src/$1" }
    ]
  }
  